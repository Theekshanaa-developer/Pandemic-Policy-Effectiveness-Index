import pandas as pd
import seaborn as sns
import matplotlib.pyplot as plt

lag_df = pd.read_csv("3_lag_optimization_ccf/outputs/lag_results.csv")
lag_df.describe()
plt.figure(figsize=(8,5))
sns.histplot(lag_df["lag_days"], bins=15, kde=True)
plt.title("Lag Distribution Across Countries")
plt.xlabel("Lag (days)")
plt.show()
display(lag_df.sort_values("lag_days").head(10))     # Shortest lags
display(lag_df.sort_values("lag_days", ascending=False).head(10))  # Longest lags
